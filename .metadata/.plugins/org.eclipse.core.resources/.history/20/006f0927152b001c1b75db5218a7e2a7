public class TimeTeste {
	Time time = new Time();
	
	@Before
    public void init(){
        time = new Time();
    }
	
    @Test
    public void testeAtacante() {
    	
    	time.set_pontos(10);
    	assertEquals(10, 10);
    	int result = time.pontos;
    	
    }
}
